script=start
	on:use { 
		//Ride when tame!
		if (gotstate("self","tame")) {
			ride;
			freetimers "self";
			if ($tameness<15){
				timer "self",random(5000,10000),1,"shakeoff";
			}elseif ($tameness<20){
				timer "self",random(20000,30000),1,"shakeoff";
			}
		}	
	}

	on:shakeoff {
		if (riding()==current_id()){
			getoff;
			damage "unit", 1,random(5,10);
			msg "Ouch ...",3;
			msg "He dropped me!",3;
		}
	}

	on:ai_attack {
		if (random(8)==1){
			if (gotstate("unit",1,17)==0){
				if (random(5)<2){
					addstate "unit",1,1;	
				}else{
					addstate "unit",1,7;
				}
			}
		}
	}	
	
	on:kill {
		event "iskill_hunt","global";
	}
script=end


