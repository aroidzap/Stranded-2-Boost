### Monkey Carrier
id=44
name=Transport Monkey
group=animal
icon=gfx\monkey_carrier.bmp
model=gfx\monkey_carrier.b3d
colxr=18
colyr=14
scale=0.51
behaviour=monkey
health=50
speed=1.4
turnspeed=1.
sfx=monkey
ani_move=3,4,0.09
ani_idle1=5,7,0.13
ani_idle2=7,14,0.17
ani_idle3=14,16,0.12
ani_die=16,17,0.15
loot=11,4
loot=94,2
loot=96,1
maxweight=10000
script=start
	on:create {
		timer "self",3000,0;
	}
	on:start {
		timer "self",3000,0;
	}
	on:timer {
		if (health("self")>0){
			ai_mode "self","goto","unit","1";
			ai_center;
		}
		if (count_stored("self")>0){
			addstate "self","particles";
		}else{
			freestate "self","particles";
		}
	}
	on:use {
		if (health("self")>0){
			exchange "self";
		}
	}
	on:kill {
		freetimers "self";
		event "iskill_hunt","global";
	}
script=end

