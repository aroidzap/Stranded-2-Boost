### Coral
id=210
name=Coral
group=stuff
icon=gfx\coral01.bmp
model=gfx\coral01.b3d
health=50
mat=leaf
fx=16
scale=3.5
col=2
swayspeed=2
swaypower=15
var=radius,Radius,400,0
var=count,Fish per Day,1,0
var=max,Fish max,1,0
script=start
	on:start {
		local $ftype;
		$ftype=4;
		if (random(0,1)==1){
			$ftype=random(11,12);
		}
	}
	on:changeday {
		loop("count",$count) {
			if (count_inrange("unit",$ftype,$radius,"self")<$max){
				create "unit",$ftype,getx("self"),getz("self");
			}
		}
	}
script=end

