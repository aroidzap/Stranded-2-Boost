### Moss
id=202
name=Moss
group=stuff
icon=gfx\moss.bmp
model=gfx\moss.b3d
health=300
col=0
mat=plant
alpha=0.35
scale=1.5
fx=16
var=max,Mushrooms max,3,0
var=max2,Snails max,1,0
script=start
	on:changeday {
		//Spawn Fungi
		if (random(2)==1){
			if (count_inrange("item",33,25,"self")<$max){
				local $xp,$zp;
				$xp=getx("self");
				$zp=getz("self");
				$xp+=random(-15,15);
				$zp+=random(-15,15);
				create "item",33,$xp,$zp;
				freevar $xp,$zp;
			}
		}
		//Spawn Snail
		if (random(3)==1){
			if (count_inrange("unit",18,300,"self")<$max2){
				create "unit",18,getx("self"),getz("self");
			}
		}
	}
script=end

