### Grain
id=121
name=Grain
group=stuff
icon=gfx\grain.bmp
model=gfx\grain.b3d
behaviour=plague_target
fx=16
scale=0.8
swayspeed=2
swaypower=5
col=2
health=45
mat=leaf
growtime=5
script=start
	on:kill {
		$tmp=1;
		$tmp2=spawntimer("self");
		if ($tmp2>-3){$tmp=2;}
		if ($tmp2>=0){$tmp=3;}
		create "item",41,getx("self"),getz("self"),$tmp;
		corona getx("self"),getz("self");
		freevar $tmp,$tmp2;
	}
	on:plant {
		spawntimer "self",-4;
	}
script=end

