### Raptornest
id=196
name=Raptor Nest
group=stuff
icon=gfx\raptornest.bmp
model=gfx\raptornest.b3d
health=100
mat=leaf
fx=16
scale=1.6
var=radius,Radius,500,0
var=count,Raptors per Day,1,0
var=max,Raptors max,1,0
script=start
	on:changeday {
		loop("count",$count) {
			if (count_inrange("unit",2,$radius,"self")<$max){
				create "unit",2,getx("self"),getz("self");
			}
		}
	}
script=end


